{
  "name": "source-flat-schema-debezium",
  "config": {
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "tasks.max": "1",
    "plugin.name": "pgoutput",
    "publication.autocreate.mode": "filtered",
    "snapshot.mode": "initial",
    "include.schema.changes": "true",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable": "true",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "true",
    "topic.creation.enable": "true",
    "decimal.handling.mode": "double",
    "heartbeat.interval.ms": 300000,
    "topic.creation.default.replication.factor": "1",
    "topic.creation.default.partitions": "1",
    "database.history.kafka.topic": "schema-changes.postgres",
    "database.hostname": "database",
    "database.port": "5432",
    "database.user": "postgres",
    "database.password": "postgres",
    "database.dbname": "source_db",
    "database.history.kafka.bootstrap.servers": "http://kafka:9092",
    "publication.name": "flat_schema_demo_publication",
    "slot.name": "flat_schema_demo_slot",
    "database.server.name": "girls-band-anime-db-flat-scema",
    "topic.prefix": "demo.flat.schema.sourcepg",
    "table.include.list": "public.characters",
    "transforms": "unwrap",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState"
  }
}